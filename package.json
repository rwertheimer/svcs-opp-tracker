{
  "name": "fivetran-services-tracker-backend",
  "version": "1.0.0",
  "description": "Backend services for the Fivetran Services Opportunity Tracker",
  "main": "server.js",
  "scripts": {
    "start:server": "nodemon --watch 'backend/**/*.ts' --exec 'ts-node -P backend/tsconfig.json backend/server.ts'",
    "seed:firestore": "npx ts-node backend/seedDatabase.ts",
    "seed:postgres": "npx ts-node backend/seedPostgres.ts",
    "dev:frontend": "vite",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^7.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pino": "^9.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^24.1.0",
    "msw": "^2.3.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
